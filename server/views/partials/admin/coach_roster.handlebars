    <table class="table table-bordered">
        <thead>
          <tr>
        {{> gm_spreadsheet/coach_headers}}
          </tr>
        </thead>
        <tbody>
            {{#each coaches}}
          <tr>  
            <td>{{this.firstname}}</td>
            <td>{{this.lastname}}</td>
            <td>{{this.team.role}}</td>
            <td>{{this.contact.email}}</td>
            <td>{{this.contact.phone1}}</td>
            <td>{{this.registration.apparel.hat}}</td>
            <td>{{this.registration.apparel.shirt}}</td>
            <td>{{this.registration.apparel.jacket}}</td>
            <td>{{this.registration.apparel.polo}}</td>
            <td>N/A</td>
            <td>N/A</td>
            <td>
                <a href="/admin/db/staff/coach/{{this._id}}">View</a>
            </td>            
          </tr>
          {{/each}}
        </tbody>
      </table> 
